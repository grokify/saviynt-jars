{
	"fullyQualifiedClassName": "com.jar.jarCS",
	"methodName": "createAccount",
	"arguments": {
		"connect_url": "${connectionJSON.url}",
		"connect_username": "${connectionJSON.username}",
		"connect_password": "${connectionJSON.password}",
		"username": "${arsTasks.getAccountName()}",
		"accountName": "${arsTasks.getAccountName()}",
		"taskType": "${arsTasks.getTasktype()}",
		"taskid": "${arsTasks.getId()}",
		"usernamedescattr": "'${users.firstname} ${users.lastname}:${users.username}'",
		"DYN_ ACGCDE": "${requestAccessAttributes.get('AccountingCode')}",
		"DYN_ SPCAUT": "${requestAccessAttributes.get('SpecialAuth').replaceAll(',',' ')}",
		"DYN_ PASSWORD": "${requestAccessAttributes.get('Password')}",
		"DYN_ SUPGRPPRF": "${requestAccessAttributes.get('SUPGRPS').replaceAll(',',' ')}",
		"DYN_ INLMNU": "${if(requestAccessAttributes.get('JOBDLIB').equals('QGPL')){requestAccessAttributes.get('InitialMenu')} else {requestAccessAttributes.get('JOBDLIB')+'/'+requestAccessAttributes.get('InitialMenu')}}",
		"DYN_ JOBD": "${requestAccessAttributes.get('JOBDLIB')}/${requestAccessAttributes.get('JobDesc')}",
		"DYN_ MSGQ": "${requestAccessAttributes.get('MSQLIB')}/${arsTasks.getAccountName()}",
		"DYN_ GRPAUTTYP": "${requestAccessAttributes.get('GRPAUTHTYPE')}",
		"DYN_ INLPGM": "${if(requestAccessAttributes.get('JOBDLIB').equals('QGPL') || requestAccessAttributes.get('InitialProgLib').equals('*NONE')) {requestAccessAttributes.get('InitialProgLib')} else {requestAccessAttributes.get('JOBDLIB')+'/'+requestAccessAttributes.get('InitialProgLib')}}",
		"DYN_ OUTQ": "${requestAccessAttributes.get('OUTQ')}",
		"DYN_ CURLIB": "${requestAccessAttributes.get('CURLIB')}",
		"DYN_ LMTCPB": "${requestAccessAttributes.get('LIMCAP')}",
		"DYN_ SRTSEQ": "${requestAccessAttributes.get('SRTSEQ')}",
		"DYN_ PRTDEV": "${requestAccessAttributes.get('PRNTDEV')}",
		"DYN_ MAXSTG": "${requestAccessAttributes.get('MAXSTORALL')}",
		"DYN_ HOMEDIR": "'/home/${arsTasks.getAccountName()}'",
		"DYN_ PWDEXP": "*YES",
		"DYN_ OWNER": "${requestAccessAttributes.get('Owner')}",
		"DYN_ ASTLVL": "${requestAccessAttributes.get('ASTLVL')}",
		"USRPRF": "${arsTasks.getAccountName()}",
		"entitlementname": "${requestAccessAttributes.get('Profile')}",
		"groupprofile": "${requestAccessAttributes.get('GroupProfile')}"
	}
}